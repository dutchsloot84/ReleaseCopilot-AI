{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ManageLogGroups",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:DeleteLogGroup",
        "logs:DeleteLogStream",
        "logs:DescribeLogGroups",
        "logs:DescribeLogStreams",
        "logs:PutLogEvents",
        "logs:PutRetentionPolicy",
        "logs:TagLogGroup"
      ],
      "Resource": [
        "arn:aws:logs:us-west-2:781047749939:log-group:/aws/lambda/releasecopilot-core:*",
        "arn:aws:logs:us-west-2:781047749939:log-group:/aws/lambda/releasecopilot-lambda:*"
      ]
    }
  ]
}
