{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "BootstrapAssets",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:GetObjectVersion",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::cdk-hnb659fds-assets-${aws:AccountId}-${aws:Region}",
        "arn:aws:s3:::cdk-hnb659fds-assets-${aws:AccountId}-${aws:Region}/*"
      ]
    }
  ]
}
